# Path to service account credentials JSON (relative to project root)
GOOGLE_CREDENTIALS_PATH=credentials.json

# Your Google Spreadsheet ID (the long id in the sheet URL)
SPREADSHEET_ID=your_spreadsheet_id_here

# The sheet name/ range to append to (default "Sheet1!A:F")
SPREADSHEET_RANGE=Sheet1!A:F

# Optional: host / port for uvicorn
UVICORN_HOST=127.0.0.1
UVICORN_PORT=8000
